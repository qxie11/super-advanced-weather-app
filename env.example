# OpenWeatherMap API key
# Get a free key at https://openweathermap.org/api
NEXT_PUBLIC_WEATHER_API_KEY=your_openweathermap_api_key_here

# Weather API base URL (optional)
NEXT_PUBLIC_WEATHER_API_BASE_URL=https://api.openweathermap.org/data/2.5

# Default language (optional)
# Available languages: en, ru, es, fr, de, it
NEXT_PUBLIC_DEFAULT_LANGUAGE=en

# Units of measurement (optional)
# Available units: metric (Celsius), imperial (Fahrenheit), kelvin (Kelvin)
NEXT_PUBLIC_UNITS=metric 